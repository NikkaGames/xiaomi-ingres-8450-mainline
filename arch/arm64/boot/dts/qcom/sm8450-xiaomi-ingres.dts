&tlmm {
	gpio-reserved-ranges = <28 4>, <36 4>;
	
	cirrus_reset_default_0: cirrus-reset-default-0 {
		pins = "gpio9";
		function = "gpio";
		power-soruce = <2>;
		input-enable;
		bias-pull-up;
	};

	cirrus_irq_default_0: cirrus-irq-default-0 {
		pins = "gpio180";
		function = "gpio";
		drive-strength = <2>;
		PIN_DT_FUNC_OUTPUT;
		bias-disable;
	};

	cirrus_irq_default_1: cirrus-irq-default-1 {
		pins = "gpio19";
		function = "gpio";
		drive-strength = <2>;
		bias-disable;
	};
	
	bt_en: bt-en-state {
		pins = "gpio81";
		function = "gpio";
		drive-strength = <16>;
		output-low;
		bias-pull-down;
	};

	wlan_en: wlan-en-state {
		pins = "gpio80";
		function = "gpio";
		drive-strength = <16>;
		bias-disable;
		output-low;
	};

	uart20_default: uart20-default-state {
		cts-pins {
			pins = "gpio76";
			function = "qup20";
			bias-disable;
		};

		rts-pins {
			pins = "gpio77";
			function = "qup20";
			bias-disable;
		};

		rx-pins {
			pins = "gpio78";
			function = "qup20";
			bias-disable;
		};

		tx-pins {
			pins = "gpio79";
			function = "qup20";
			bias-disable;
		};
	};
	
	i2s1_default: i2s1-default-state {
		sck-pins {
			pins = "gpio6";
			function = "i2s1_sck";
			drive-strength = <8>;
			bias-disable;
			output-high;
		};

		data0-pins {
			pins = "gpio8";
			function = "i2s1_data0";
			drive-strength = <8>;
			bias-disable;
		};

		ws-pins {
			pins = "gpio7";
			function = "i2s1_ws";
			drive-strength = <8>;
			bias-disable;
			output-high;
		};

		data1-pins {
			pins = "gpio9";
			function = "i2s1_data1";
			drive-strength = <8>;
			bias-disable;
			output-high;
		};
	};
	
	xo_clk_default: xo-clk-state {
		pins = "gpio204";
		function = "gpio";
		drive-strength = <16>;
		output-low;
		bias-pull-down;
	};
	
	wcd_default: wcd-reset-n-active-state {
		pins = "gpio43";
		function = "gpio";
		drive-strength = <10>;
		bias-disable;
		output-low;
	};
};

&i2c15 {
	clock-frequency = <1000000>;
	status = "okay";

	cs35l41_b: cs35l43@40 {
		compatible = "cirrus,cs35l41";
		reg = <0x40>;
		interrupt-parent = <&tlmm>;
		interrupts = <19 IRQ_TYPE_LEVEL_LOW>;
		reset-gpios = <&tlmm 183 GPIO_ACTIVE_HIGH>;
		#sound-dai-cells = <1>;
		sound-name-prefix = "B";
		cirrus,boost-type = <0>;
		cirrus,boost-peak-milliamp = <4100>;
		cirrus,boost-ind-nanohenry = <1000>;
		cirrus,boost-cap-microfarad = <15>;
		cirrus,asp-sdout-hiz = <3>;
		cirrus,gpio2-output-enable;
		cirrus,gpio2-src-select = <2>;
	};

	cs35l41_t: cs35l43@42 {
		compatible = "cirrus,cs35l41";
		reg = <0x42>;
		interrupt-parent = <&tlmm>;
		interrupts = <19 IRQ_TYPE_LEVEL_LOW>;
		reset-gpios = <&tlmm 182 GPIO_ACTIVE_HIGH>;
		#sound-dai-cells = <1>;
		sound-name-prefix = "T";
		cirrus,boost-type = <0>;
		cirrus,boost-peak-milliamp = <4100>;
		cirrus,boost-ind-nanohenry = <1000>;
		cirrus,boost-cap-microfarad = <15>;
		cirrus,asp-sdout-hiz = <3>;
		cirrus,gpio2-output-enable;
		cirrus,gpio2-src-select = <2>;
	};
};

